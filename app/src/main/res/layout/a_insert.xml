                android:src="@drawable/a_06_exercise"
                android:layout_weight="1"
                android:padding="10dp"
                android:scaleType="fitCenter"
                android:background="?selectableItemBackground"/>
            <ImageButton
                android:id="@+id/A00Switch003"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/a_03_book"
                android:layout_weight="1"
                android:scaleType="fitCenter"
                android:background="?selectableItemBackground"/>
            <ImageButton
                android:id="@+id/A00Switch004"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/a_08_entertainment"
                android:layout_weight="1"
                android:scaleType="fitCenter"
                android:padding="10dp"
                android:background="?selectableItemBackground"/>
        </LinearLayout>

        <LinearLayout
