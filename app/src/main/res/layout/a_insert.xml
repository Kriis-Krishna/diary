                    android:id="@+id/InsertEditTextGeneric"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-condensed"
                    android:textSize="14dp"
                    android:padding="5dp"
                    android:textColor="@color/grey_001"
                    android:background="?android:colorBackground"
                    android:maxLength="512"
                    android:maxHeight="150dp"
                    android:hint="Happiness is acceptance ..."/>
            </com.google.android.material.textfield.TextInputLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:fontFamily="sans-serif-condensed"
                android:gravity="center_vertical"
                android:text="     Today's Rating"
                android:textColor="@color/grey_001"
                android:drawableStart="@drawable/a_17_notes"
                android:textSize="12dp" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/A00_Star000"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:button="@drawable/star_switch_10"/>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/A00_Star001"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:button="@drawable/star_switch_10"/>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/A00_Star002"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:button="@drawable/star_switch_10"/>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-condensed"
                android:gravity="center"
                android:padding="10dp"
                android:text="Keep track of your days rating by clicking the star"
                android:textColor="@color/grey_001"
                android:textSize="12dp" />
        </LinearLayout>

<!--        <TextView-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="wrap_content"-->
<!--            android:padding="14dp"-->
<!--            android:fontFamily="sans-serif-condensed"-->
<!--            android:gravity="center_vertical"-->
<!--            android:text="  Today's Activity"-->
