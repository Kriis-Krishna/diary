                android:layout_height="2dp"
                android:layout_weight="1"
                android:visibility="invisible"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/grey_002"/>
            <View
                android:id="@+id/A00_Icon_003"
